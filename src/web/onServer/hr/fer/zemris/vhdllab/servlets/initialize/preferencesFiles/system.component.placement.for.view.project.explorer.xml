<?xml version="1.0" encoding="UTF-8"?>

<property id="system.component.placement.for.view.project.explorer" type="system">
	<description>Project explorer placement</description>
	<tooltip>The default location where project explorer will open</tooltip>
	<editable byUser="true" />
	<data type="string" editor="list">
		<!-- These values must be all uppercase because they are liked to enum -->
		<values>
			<value>LEFT</value>
			<value>RIGHT</value>
			<value>BOTTOM</value>
			<value>CENTER</value>
		</values>
		<default>LEFT</default>
	</data>
</property>