<?xml version="1.0" encoding="UTF-8"?>
<assembly>
  <id>bin</id>
  <formats>
    <format>war</format>
  </formats>
  <includeBaseDirectory>false</includeBaseDirectory>
  <moduleSets>
    <moduleSet>
      <includes>
        <include>hr.fer.zemris.vhdllab:vhdllab-server</include>
      </includes>
      <binaries>
        <outputFileNameMapping>${module.artifactId}-${module.version}.${module.extension}</outputFileNameMapping>
        <outputDirectory>/WEB-INF/lib/</outputDirectory>
        <unpack>false</unpack>
      </binaries>
    </moduleSet>
  </moduleSets>
  <fileSets>
    <fileSet>
      <directory>vhdllab-server/target/classes</directory>
      <includes>
        <include>web.xml</include>
        <include>parameters.properties</include>
        <include>launch.jnlp</include>
      </includes>
      <outputDirectory>/WEB-INF/</outputDirectory>
    </fileSet>
  </fileSets>
  <files>
    <file>
      <source>vhdllab-server/src/main/resources/log4j.properties</source>
      <outputDirectory>/WEB-INF/classes/</outputDirectory>
    </file>
    <file>
      <source>vhdllab-client/target/vhdllab-client-${pom.version}-all.jar</source>
      <outputDirectory></outputDirectory>
    </file>
  </files>
</assembly>
