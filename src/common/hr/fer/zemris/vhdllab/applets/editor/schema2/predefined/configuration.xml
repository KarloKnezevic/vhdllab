<?xml version="1.0" encoding="UTF-8"?>

<componentList>
   <component>
        <componentName>AND_gate</componentName>
        <codeFileName>AND_gate.vhdl</codeFileName>
        <drawerName>hr.fer.zemris.vhdllab.onClient.applets.schema2.model.drawers.DefaultComponentDrawer</drawerName>
        <categoryName>BasicGates</categoryName>
		<preferredName>AND_gate_instance</preferredName>
        <genericComponent>true</genericComponent>

        <parameterList>
           <parameter>
              <generic>true</generic>
              <paramType>TIME</paramType>
              <name>delay</name>
              <value>0 ns</value>
              <allowedValues>
              </allowedValues>
           </parameter>

           <parameter>
              <generic>true</generic>
              <paramType>INTEGER</paramType>
              <name>input_port_num</name>
              <value>2</value>
              <allowedValues>
              </allowedValues>
           </parameter>
        </parameterList>
       
        <portList>
           <port>
              <name>input_ports</name>
              <orientation>WEST</orientation>
              <direction>IN</direction>
              <type>std_logic_vector</type>
              <vectorAscension>ascend</vectorAscension>
              <lowerBound>0</lowerBound>
              <upperBound>1</upperBound>
           </port>

           <port>
              <name>output_port</name>
              <orientation>EAST</orientation>
              <direction>OUT</direction>
              <type>std_logic</type>
           </port>
        </portList>
   </component>
</componentList>


