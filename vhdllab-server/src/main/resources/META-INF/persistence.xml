<?xml version='1.0' encoding='UTF-8'?>
<persistence xmlns="http://java.sun.com/xml/ns/persistence"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://java.sun.com/xml/ns/persistence
     http://java.sun.com/xml/ns/persistence/persistence_1_0.xsd"
  version="1.0">

  <persistence-unit name="vhdllab" transaction-type="RESOURCE_LOCAL">
    <!--    <mapping-file>META-INF/orm.xml</mapping-file>-->
    <class>hr.fer.zemris.vhdllab.entity.File</class>
    <class>hr.fer.zemris.vhdllab.entity.Project</class>
    <class>hr.fer.zemris.vhdllab.entity.FileHistory</class>
    <class>hr.fer.zemris.vhdllab.entity.ProjectHistory</class>
    <class>hr.fer.zemris.vhdllab.entity.ClientLog</class>
    <exclude-unlisted-classes />
  </persistence-unit>
</persistence>
