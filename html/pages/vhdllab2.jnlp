<?xml version="1.0" encoding="utf-8"?>
<!-- JNLP File for VHDLLab -->

<jnlp spec="1.6+" codebase="http://localhost:8080/vhdllab/" href="vhdllab2.jnlp">
   <information>
      <title>VHDLLab application</title>
      <vendor>The FER java team</vendor>
      <description>VHDLLab</description>
      <homepage href="http://java.sun.com/docs/books/tutorial/deployment/webstart/running.html"/>
      <description kind="short">VHDLLab</description>
   </information>
   <shortcut online="true">
      <desktop/>
      <menu submenu="My VHDLLab App"/>
    </shortcut>
    <update check="always" policy="prompt-update" />
   <resources>     
   	<jar href="vhdllab-web-onClient.jar"/>   
   	<jar href="commons-logging.jar"/>   
   	<jar href="commons-collections.jar"/>   
   	<jar href="commons-beanutils-1.7.0.jar"/>   
   	<jar href="commons-digester-1.7.jar"/>   
   	<jar href="commons-codec-1.3.jar"/>   
   	<jar href="commons-httpclient-3.0.1.jar"/>   
	<j2se version="1.6+"
	      href="http://java.sun.com/products/autodl/j2se"/>
   </resources>
   <application-desc main-class="hr.fer.zemris.vhdllab.applets.main.MainFrame">
</jnlp>